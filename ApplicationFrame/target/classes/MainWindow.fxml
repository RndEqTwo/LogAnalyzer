<?xml version="1.0" encoding="UTF-8"?>

<?import de.jduwe.loganalyzer.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox prefHeight="720.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.jduwe.loganalyzer.MainWindowController">
    <!--<TextArea fx:id="logFileTextArea" editable="false" minWidth="-Infinity" HBox.hgrow="ALWAYS" /> -->
    <LogListView fx:id="logListView" minWidth="-Infinity" HBox.hgrow="ALWAYS" />
    <VBox>
        <HBox fx:id="ImportButtonRow" alignment="CENTER">
            <Button onAction="#loadFromFile" text="Load From File" />
            <Button text="Load From Clipboard" />
            <Button text="Settings" />
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
        </HBox>
        <HBox fx:id="FilterSettings" prefHeight="200.0" style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;">
            <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
                <FlowPane fx:id="activePluginChips" minHeight="40.0">
                   <padding>
                      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                   </padding>
                </FlowPane>
                <Pane fx:id="PluginSettingsArea" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS" />
                <HBox fx:id="PluginActions" alignment="CENTER" spacing="20.0">
                    <Button fx:id="FindButton" onAction="#findButtonClicked" text="Find" />
                    <Button fx:id="FilterButton" onAction="#filterButtonClicked" text="Filter" />
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
                </HBox>
            </VBox>
            <VBox fx:id="AvailablePluginsList" maxWidth="150.0" minWidth="-Infinity" style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;">
            </VBox>
        </HBox>
        <Pane fx:id="FinderShowArea" VBox.vgrow="ALWAYS" />

    </VBox>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>

</HBox>
